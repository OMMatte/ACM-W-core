{
  "name": "acm-w-game",
  "version": "1.0.0",
  "dependencies": {},
  "devDependencies": {
    "babel-preset-es2015": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "eslint": "^3.8.1",
    "jasmine": "^2.5.2",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-browserify": "^5.1.0",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-diff-reporter": "^0.6.2",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "node-sass": "^3.10.1",
    "nodemon": "^1.11.0",
    "phantomjs": "^2.1.7",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "lint": "eslint src",
    "test": "karma start --single-run",
    "watch:lint": "nodemon -q -w src/ --ext \".*\" --exec \"npm run lint\"",
    "watch:test": "karma start",
    "watch:test:lint": "nodemon -q -w src/ --ext \".*\" --exec \"npm run test && npm run lint\"",
    "watch": "nodemon -q -w src/ --ext \".*\" --exec \"npm run test\""
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
